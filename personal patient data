# data_ingestion/patient_data_ingestion.py

import pandas as pd

def ingest_patient_data(file_path):
    """Ingest personal patient data from a CSV file."""
    return pd.read_csv(file_path)

# Example usage
# patient_data = ingest_patient_data('path_to_patient_data.csv')

#Sample CSV Structure (patient_data.csv):
patient_id, name, age, gender, contact_info
1, John Doe, 34, M, johndoe@example.com
2, Jane Smith, 28, F, janesmith@example.com
